w (Row 11) -->\n    <tr>\n        <th style=\"padding: 6px;\">TOTALS</th>\n        <td style=\"background: #e6f0e6;\">&nbsp;</td>\n        <td style=\"background: #e6f0e6;\">&nbsp;</td>\n        <td style=\"background: #e6f0e6;\">&nbsp;</td>\n        <td style=\"background: #e6f0e6;\">&nbsp;</td>\n        <td style=\"background: #e6f0e6;\">&nbsp;</td>\n    </tr>\n</tbody></table>\n"
